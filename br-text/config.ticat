help = setup br-text config
tags = config
abbr = conf

[arg]
backup-dir|dir|d = ''
threads|thread|t|T = ''
cluster-name|cluster|name|n|N = ''
check-checksum|checksum = true
target-db|target|db = ''

[arg2env]
